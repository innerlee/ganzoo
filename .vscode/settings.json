{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        ".vscode": true,
        ".gitignore": true,
        "**/runs/": true,
        "**/out/": true,
        "mpl": true,
        "np": true,
        "**/temp*.py": true,
        "**/_cache*": true,
        "tf": true,
        "**/*.aux": true,
        "**/*.lof": true,
        "**/*.log": true,
        "**/*.lot": true,
        "**/*.fls": true,
        "**/*.out": true,
        "**/*.toc": true,
        "**/*.fmt": true,
        "**/*.fot": true,
        "**/*.cb": true,
        "**/*.cb2": true,
        "**/*.bbl": true,
        "**/*.bcf": true,
        "**/*.blg": true,
        "**/*-blx.aux": true,
        "**/*-blx.bib": true,
        "**/*.brf": true,
        "**/*.run.xml": true,
        "**/*.fdb_latexmk": true,
        "**/*.synctex": true,
        "**/*.synctex.gz": true,
        "**/*.synctex.gz(busy)": true,
        "**/*.pdfsync": true,
        "**/*.alg": true,
        "**/*.bst": true,
        ".cache": true,
        "build": true,
        "dist": true,
        "*.egg-info": true,
        "*.enc": true,
        "*.in": true,
        "setup.cfg": true,
        "setup.py": true,
        "requirements.txt": true,
        ".travis.yml": true,
        "**/.eggs": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/node_modules/**": true,
        "**/runs/**": true,
        "**/out/**": true,
        "**/data/**": true,
        "**/_build/**": true,
        "**/samples/**": true
    },
    "python.formatting.provider": "yapf"
}
